{
  "name": "react-sharing",
  "version": "0.1.0",
  "description": "Simple react social media sharing.",
  "main": "dist/react-sharing.min.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "start": "cd example && yarn start",
    "build": "webpack -p --progress --config webpack.config.js",
    "lint": "prettier --write src/index.js example/*.js *.js",
    "now-build": "cd example && yarn && yarn build",
    "test": "yarn lint && echo \"Error: no test specified\"",
    "deploy": "cd example && yarn predeploy && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZYSzys/react-sharing.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "share"
  ],
  "author": "ZYSzys <17367077526@163.com> (https://zyszys.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZYSzys/react-sharing/issues"
  },
  "homepage": "https://github.com/ZYSzys/react-sharing#readme",
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "node-sass": "4.11.0",
    "prettier": "1.16.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.4",
    "webpack-cli": "3.2.3"
  }
}
